<h1> Companies: index </h1>

<p> Here are all of the companies: </p>

<%
=begin %>
<ul>
	<% @companies.each do |company| %>
		<li><%= link_to company.name, company %></li>
	<% end %>
</ul>
<%
=end %>

<table>
	<tbody>
		<tr>
			<th>Company Name</th>
		</tr>
		<%= render partial: "company", collection: @companies %>
	</tbody>
</table>





