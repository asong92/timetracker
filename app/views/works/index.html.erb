<h1> Works: index </h1>

<p> Here are all of the works: </p>

<%
=begin %>
<ul>
	<% @works.each do |work| %>
		<li><%= link_to work, work %></li>
	<% end %>
</ul>
<%
=end %>
<table>
	<tbody>
		<tr>
			<th>View</th>
			<th>User</th>
			<th>Project</th>
			<th>Date and Time</th>
			<th>Hours</th>
		</tr>
		<%= render partial: "work", collection: @works %>

	</tbody>
</table>

<%= will_paginate @posts %>

